<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.module.category" id="module_category_l10n_ar">
            <field name="name">Argentine Localization</field>
            <field name="description">Argentine Localization</field>
            <field name="sequence">1</field>
            <field name="parent_id" ref="base.module_category_hidden"/>
        </record>

        <record id="group_l10n_ar_invoice_user" model="res.groups" context="{'noadmin':True}">
            <field name="name">Argentine Invoice User</field>
            <field name="category_id" ref="base.module_category_accounting_and_finance"/>
            <field name="implied_ids" eval="[(4, ref('account.group_account_invoice'))]"/>
        </record>

        <record id="group_l10n_ar_invoice_admin" model="res.groups">
            <field name="name">Argentine Localization Manager</field>
            <field name="category_id" ref="module_category_l10n_ar"/>
            <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
        </record>

    </data>
</openerp>

